{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "arrowFunctions": true,
    "destructuring": true,
    "blockBindings": true,
    "classes": true,
    "binaryLiterals": true,
    "spread": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "octalLiterals": true,
    "defaultParams": true,
    "regexYFlag": true,
    "regexUFlag": true,
    "superInFunctions": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true,
    "restParams": true,
    "globalReturn": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "comma-spacing": 0,
    "strict": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "semi-spacing": 1,
    "space-infix-ops": 1,
    "quotes": [1,"single"],
    "no-use-before-define": 1,
    "key-spacing": 1,
    "new-cap": 1,
    "no-path-concat": 0,
    "no-unused-expressions": 0,
    "space-infix-ops": 0,
    "no-trailing-spaces": 1,
    "eol-last": 1,
    "no-process-exit": 0,
    "semi": 1,
    "key-spacing": 0,
    "no-var": 0
  }
}
